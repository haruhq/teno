{
  "name": "teno",
  "version": "0.0.1-alpha.0",
  "description": "Best rich text editor written in TypeScript.",
  "keywords": [
    "editor",
    "memo",
    "note",
    "rich",
    "rich text",
    "text",
    "typescript",
    "wysiwyg"
  ],
  "license": "MIT",
  "author": "Lucas Choi <tetrtiz.com@gmail.com>",
  "files": [
    "lib"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tetritz/mul.git"
  },
  "scripts": {
    "clean": "rimraf lib/*",
    "babel": "babel src --out-dir lib --extensions .ts",
    "start": "node ./lib/index.js",
    "git:push": "git add . && git commit -m 'Update codes' && git push",
    "tsc": "tsc",
    "w": "babel --watch src --out-dir lib --extensions .ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@devtree/base-pack": "^0.0.5",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "chalk": "^4.1.0",
    "lodash": "^4.17.20",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {},
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "node": "12.15.0"
          }
        }
      ]
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 5%",
    "not ie <= 10"
  ],
  "eslintConfig": {
    "extends": [
      "node_modules/@devtree/base-pack/eslint/base.js"
    ]
  },
  "prettier": "@devtree/base-pack/prettier",
  "sideEffects": false,
  "types": "lib/index.d.ts"
}
